{
    "compilerOptions": {
      "target": "es2022",
      "module": "es2022",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitReturns": true,
      "rootDir": "src",
      "outDir": "dist",
      "skipLibCheck": true,
      "sourceMap": true,
      "noFallthroughCasesInSwitch": true,
      "noEmitOnError": true,
      "pretty": true
    }
  }
